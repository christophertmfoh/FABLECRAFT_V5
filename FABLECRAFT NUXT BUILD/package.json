{
  "name": "fablecraft-foundation",
  "private": true,
  "type": "module",
  "imports": {
    "#internal/nuxt/paths": "./node_modules/nuxt/dist/core/runtime/nitro/utils/paths.js"
  },
  "_SAFETY_WARNING": "NEVER run 'kill node' or 'pkill node' - Use Ctrl+C to stop processes safely",
  "scripts": {
    "init": "./AGENT_INIT.sh",
    "predev": "./AGENT_INIT.sh",
    "safe-stop": "./scripts/safe-stop.sh",
    "list-dev": "./scripts/list-dev-servers.sh",
    "dev": "echo '⚠️  REMINDER: Use Ctrl+C to stop the server, NEVER use killall/pkill node! ⚠️' && npx nuxi dev",
    "dev:alt": "echo '⚠️  REMINDER: Use Ctrl+C to stop the server, NEVER use killall/pkill node! ⚠️' && npx nuxi dev --port 3050",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "pnpm prettier:fix && pnpm lint:fix",
    "fix-console": "node scripts/fix-console-logs.cjs",
    "typecheck": "nuxt typecheck",
    "typecheck:skip-lib": "vue-tsc --noEmit --skipLibCheck",
    "clean": "rm -rf .nuxt .output dist",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:diff": "supabase db diff",
    "db:types": "supabase gen types typescript --local > types/supabase.ts",
    "db:status": "supabase status",
    "prepare": "husky",
    "analyze": "nuxt build --analyze",
    "analyze:prod": "NODE_ENV=production nuxt build --analyze",
    "build:prod": "NODE_ENV=production nuxt build && nuxt preview",
    "performance": "nuxt build && npx nuxi analyze",
    "lighthouse": "npm run build:prod & sleep 10 && npx lighthouse http://localhost:3000 --only-categories=performance --output=json --output-path=lighthouse-report.json",
    "test:cache-headers": "curl -I http://localhost:3000/ && curl -I http://localhost:3000/_nuxt/",
    "analyze:css": "nuxt build --analyze && echo 'CSS Analysis Complete'",
    "css:size": "npm run build && find .nuxt/dist -name '*.css' -exec wc -c {} + | sort -n",
    "css:gzip": "npm run build && find .nuxt/dist -name '*.css' -exec gzip -c {} \\; | wc -c"
  },
  "dependencies": {
    "@iconify-json/lucide": "1.2.60",
    "@nuxt/devtools": "1.6.0",
    "@nuxt/icon": "1.15.0",
    "@nuxtjs/supabase": "1.6.0",
    "@pinia/nuxt": "0.11.2",
    "@vueuse/core": "13.6.0",
    "@vueuse/nuxt": "13.6.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "h3-compression": "0.3.2",
    "nuxt": "3.18.0",
    "tailwind-merge": "3.3.1",
    "vue": "3.5.18",
    "vue-router": "4.5.1",
    "web-vitals": "5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/kit": "3.18.0",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/vue": "8.1.0",
    "@types/node": "20.19.9",
    "@typescript-eslint/parser": "8.39.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/test-utils": "2.4.6",
    "cssnano": "7.1.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "glob": "11.0.3",
    "happy-dom": "^15.11.7",
    "husky": "9.1.7",
    "lighthouse": "12.8.1",
    "prettier": "3.6.2",
    "supabase": "2.33.9",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.2",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.5",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
